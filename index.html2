<!DOCTYPE html />
<html ng-app="gophervids">
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1" />

  <link href="/atom.xml" type="application/atom+xml" rel="alternate" title="Gophervids ATOM Feed" />
  <link rel="alternate" type="application/rss+xml" title="Gophervids RSS Feed" href="/rss.xml" />

  <!-- // <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>
  // <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" />
  // <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script> -->

  <script src="/static/jquery.min.js"></script>
  <link href="/static/bootstrap.min.css" rel="stylesheet" />
  <script src="/static/bootstrap.min.js"></script>

  <link href="/static/restyle.css" rel="stylesheet" />

  <!-- <link href="http://vjs.zencdn.net/4.12/video-js.css" rel="stylesheet"> -->
  <!-- // <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.13/angular.min.js"></script> -->

  <link href="/static/video-js.css" rel="stylesheet">
  <script src="/static/angular.min.js"></script>
  <script src="/static/vids.js"></script>

  <!-- // <script src="http://vjs.zencdn.net/4.12/video.js"></script> -->
  <script src="/static/video.js"></script>

  <script src="/static/vjs.youtube.js"></script>
  <script src="/static/vjs.vimeo.js"></script>
</head>
<body ng-controller="GopherTVController">
    <header class="brand_header ">
      <div class="page">
        <section class="brand_logo columns">
          <a href="/index.html"><img class="scale" src="/static/gophers.svg" /></a>
        </section><section class="brand_menu columns">
            <nav class="desktop_nav_menu">
              <ul>
                <li><a href="https://gophers.slacks.com">Community</a></li>
                <li><a href="#">ReachUs</a></li>
              </ul>
            </nav>
        </section>
      </div>
    </header>

    <section class="video_section">

      <section id="video_page" class="columns video_page">
        <div class="page">
          <div class="video_container">
            <h3 ng-bind="currentPlaylist[currentIndex].title"></h3>
            <go-video playlist="currentPlaylist" index="currentIndex" max-width="640" height="360"/>
            <br/>
          </div>
        </div>
        <!-- <div class="clear"></div> -->
      </section>

      <section id="filter_lists" class="columns filter_lists">
        <div class="page">
          <ul class="">
            <li class="activeMenu">
              <a href="" class="gs" ng-click="clear()">Getting Started With Go</a>
            </li>
            <li>
              <a href="" ng-click="recentTalks()">All Videos</a>
            </li>
            <li>
              <a href="" ng-click="newVideos()">New Videos</a>
            </li>
            <li>
              <a href="" ng-click="recentTalks()">Recent Talks</a>
            </li>
            <li class="dropdown">
              <!-- <a href="" ng-click="#">Speakers</a> -->
              <a href="" class="dropdown-toggle" data-toggle="dropdown">Video Tags <b class="caret"></b></a>
              <ul class="dropdown-menu">
                  <li ng-repeat="tag in tags">
                      <a href="" ng-click="playTag(tag.item)"><span class="badge pull-right">{{tag.count}}</span>{{tag.item}}</a>
                  </li>
              </ul>
            </li>
            <li class="dropdown">
              <a href="" class="dropdown-toggle" data-toggle="dropdown">Speakers <b class="caret"></b></a>
              <ul class="dropdown-menu">
                  <li ng-repeat="speaker in speakers">
                      <a href="" ng-click="playSpeaker(speaker.item)"><span class="badge pull-right">{{speaker.count}}</span>{{speaker.item}}</a>
                  </li>
              </ul>
            </li>
          </ul>
        </div>
        <!-- <div class="clear"></div> -->
      </section>

      <section class="columns video_lists">
        <div class="page">
            <ul>
              <li ng-repeat="video in sidebarVideoList() | filter:searchText">
                <a class="" ng-class="{'active':(video.id==currentPlaylist[currentIndex].id)}" ng-click="playVideo(video.id)">{{video.title}}</a>
              </li>
            </ul>
        </div>
      </section>

      <section class="mobile list_video_tags">
        <div class="page">
          <ul>
              <li ng-repeat="tag in tags">
                  <a href="" ng-click="playTag(tag.item); showFiltered();"><span class="badge pull-right">{{tag.count}}</span>{{tag.item}}</a>
              </li>
          </ul>
        </div>
      </section>

      <section class="mobile list_speaker_tags">
        <div class="page">
          <ul>
            <li ng-repeat="speaker in speakers">
                <a href="" ng-click="playSpeaker(speaker.item); showFiltered();"><span class="badge pull-right">{{speaker.count}}</span>{{speaker.item}}</a>
            </li>
          </ul>
        </div>
      </section>


      <section id="mobile_menu_sectors" class="mobile">
        <div class="filter" ng-show="videoExt">
          <div class="form-group">
              <input ng-focus="loadTalks()" type="text" class="form-control" placeholder="Title Search" ng-model="searchText.title">
          </div>
        </div>
        <ul>
          <li ng-click="clearMobile()"><a href="#">Basics</a></li>
          <li ng-click="recentMobileTalks()"><a href="#">Recent</a></li>
          <li ng-click="newMobileVideos()"><a href="#">New</a></li>
          <li ng-click="speakerTags()"><a href="#">People</a></li>
          <li ng-click="videoTags()"><a href="#">Tags</a></li>
        </ul>
        <div class="clear"></div>
      </section>


    </section>


</body>
</html>
